<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.blog.vuemanger.mapper.BlogTypeMapper">

    <select id="searchTypeName" resultType="java.lang.Integer">
        SELECT blog_type from blogtype where blog_typename=#{typeName}
    </select>
</mapper>
